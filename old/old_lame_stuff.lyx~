\begin_body

\begin_layout Subsection
Another cute lemma?
\end_layout

\begin_layout Subsubsection*
Lemma 1
\end_layout

\begin_layout Standard
Suppose 
\begin_inset Formula $I:\mathcal{G}\mapsto[0,\infty)$
\end_inset

 is a pseudonorm.
 So 
\begin_inset Formula $I(g)+I(f)\ge I(g+f)$
\end_inset

 and 
\begin_inset Formula $I(cg)=cI(g)\forall c>0$
\end_inset

.
 Then 
\begin_inset Formula 
\begin{eqnarray*}
\left|\frac{\partial}{\partial t}I(g+th)\right| & \le & I(h)
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard

\series bold
Proof:
\end_layout

\begin_layout Standard
By the triangle inequality for 
\begin_inset Formula $I$
\end_inset

, we have 
\begin_inset Formula 
\[
\left|I(g+th)-I(g)\right|\le I(th)
\]

\end_inset


\end_layout

\begin_layout Standard
then diving by 
\begin_inset Formula $t$
\end_inset

 and taking the limit, we have 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\left|\frac{\partial}{\partial t}I(g+th)\right|=\lim_{t\rightarrow0}\frac{\left|I(g+th)-I(g)\right|}{t}\le\lim_{t\rightarrow0}\frac{I(th)}{t}=I(h)
\]

\end_inset


\end_layout

\begin_layout Subsubsection*
Lemma 2
\end_layout

\begin_layout Standard
Suppose the conditions in lemma 1, as well as 
\begin_inset Formula 
\[
I^{v}(g)\le M\|g\|_{n}^{2}+M_{0}
\]

\end_inset


\end_layout

\begin_layout Standard
For any 
\begin_inset Formula $\lambda<\tilde{\lambda}$
\end_inset

, 
\begin_inset Formula 
\[
\left\Vert \hat{g}_{\tilde{\lambda}}-\hat{g}_{\lambda}\right\Vert _{n}^{2}=O_{p}(1)v\tilde{\lambda}^{2}I^{v}(\hat{g}_{\lambda})
\]

\end_inset


\end_layout

\begin_layout Standard

\series bold
Proof:
\end_layout

\begin_layout Standard
Since 
\begin_inset Formula $\hat{g}_{\lambda}$
\end_inset

 is the minimizer of the penalized criterion, then for 
\begin_inset Formula $h$
\end_inset

 s.t.
 
\begin_inset Formula $\hat{g}_{\lambda}+th\in\mathcal{G}\forall t>0$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial}{\partial t}\left.\left(\|y-(\hat{g}_{\lambda}+th)\|_{n}^{2}+\lambda^{2}I^{v}(\hat{g}_{\lambda}+th)\right)\right|_{t=0}=0
\]

\end_inset


\end_layout

\begin_layout Standard
But we also know that 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
\frac{\partial}{\partial t}\left.\left(\|y-(\hat{g}_{\lambda}+th)\|_{n}^{2}+\lambda^{2}I(\hat{g}_{\lambda}+th)\right)\right|_{t=0} & = & -2\left(y-\hat{g}_{\lambda},h\right)_{T}+\lambda^{2}vI^{v-1}(\hat{g}_{\lambda})\frac{\partial}{\partial t}I(\hat{g}_{\lambda}+th)
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
Then for 
\begin_inset Formula $\lambda<\tilde{\lambda}$
\end_inset

, we have
\begin_inset Formula 
\[
-2\left(y-\hat{g}_{\lambda},h\right)+\lambda^{2}vI^{v-1}(\hat{g}_{\lambda})\frac{\partial}{\partial t}I(\hat{g}_{\lambda}+th)=-2\left(y-\hat{g}_{\tilde{\lambda}},h\right)_{T}+\tilde{\lambda}^{2}vI^{v-1}(\hat{g}_{\tilde{\lambda}})\frac{\partial}{\partial t}I(\hat{g}_{\tilde{\lambda}}+th)
\]

\end_inset


\end_layout

\begin_layout Standard
Rearranging, we get 
\begin_inset Formula 
\begin{eqnarray*}
0 & = & 2\left(\hat{g}_{\tilde{\lambda}}-\hat{g}_{\lambda},h\right)_{T}+v\left(\tilde{\lambda}^{2}I^{v-1}(\hat{g}_{\tilde{\lambda}})\frac{\partial}{\partial t}I(\hat{g}_{\tilde{\lambda}}+th)-\lambda^{2}I^{v-1}(\hat{g}_{\lambda})\frac{\partial}{\partial t}I(\hat{g}_{\lambda}+th)\right)\\
 & \le & 2\left(\hat{g}_{\tilde{\lambda}}-\hat{g}_{\lambda},h\right)_{T}+vI^{v-1}(\hat{g}_{\lambda})I(h)\tilde{\lambda}^{2}
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
where the first inequality follows from Lemma 1.
 Setting 
\begin_inset Formula $h=\hat{g}_{\lambda}-\hat{g}_{\tilde{\lambda}}$
\end_inset

, we get
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
\left\Vert \hat{g}_{\tilde{\lambda}}-\hat{g}_{\lambda}\right\Vert _{n}^{2} & = & v\tilde{\lambda}^{2}I^{v-1}(\hat{g}_{\lambda})I\left(\hat{g}_{\tilde{\lambda}}-\hat{g}_{\lambda}\right)
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
Since 
\begin_inset Formula 
\[
I\left(\hat{g}_{\tilde{\lambda}}-\hat{g}_{\lambda}\right)\le I(\hat{g}_{\tilde{\lambda}})+I(\hat{g}_{\lambda})
\]

\end_inset


\end_layout

\begin_layout Standard
then 
\begin_inset Formula 
\[
\left\Vert \hat{g}_{\tilde{\lambda}}-\hat{g}_{\lambda}\right\Vert _{n}^{2}=O_{p}(1)v\tilde{\lambda}^{2}I^{v}(\hat{g}_{\lambda})
\]

\end_inset


\end_layout

\begin_layout Standard
(Note that if we assume 
\begin_inset Formula $I^{v}\left(\hat{g}_{\tilde{\lambda}}-\hat{g}_{\lambda}\right)\le O_{P}(1)\left\Vert \hat{g}_{\tilde{\lambda}}-\hat{g}_{\lambda}\right\Vert _{n}^{2}+C$
\end_inset

, then 
\begin_inset Formula $\left\Vert \hat{g}_{\tilde{\lambda}}-\hat{g}_{\lambda}\right\Vert _{n}^{2-2/v}=O_{p}(1)vI^{v-1}(\hat{g}_{\lambda})\tilde{\lambda}^{2}+C$
\end_inset

.)
\end_layout

\begin_layout Subsubsection*
Lemma 3
\end_layout

\begin_layout Standard
Suppose 
\begin_inset Formula 
\[
H\left(\delta,\left\{ g:I(g)\le1\right\} ,\|\cdot\|_{n}\right)\le A\delta^{-\alpha}
\]

\end_inset


\end_layout

\begin_layout Standard
and 
\begin_inset Formula 
\[
\frac{\|g\|_{n}}{I(g)}\le K<\infty
\]

\end_inset


\end_layout

\begin_layout Standard
Suppose for some 
\begin_inset Formula $\hat{\lambda}\le\tilde{\lambda}$
\end_inset

, 
\begin_inset Formula $\left\Vert \hat{g}_{\tilde{\lambda}}-\hat{g}_{\hat{\lambda}}\right\Vert _{n}$
\end_inset

 is bounded (doesn't grow with 
\begin_inset Formula $n$
\end_inset

).
\end_layout

\begin_layout Standard
Suppose 
\begin_inset Formula 
\[
I^{v}(\hat{g}_{\hat{\lambda}})\le M\|\hat{g}_{\hat{\lambda}}\|_{n}^{2}+M_{0}
\]

\end_inset


\end_layout

\begin_layout Standard
(Note: this is probably not necessary, but I'm sleepy)
\end_layout

\begin_layout Standard
Then 
\begin_inset Formula 
\[
H\left(\delta,\left\{ \hat{g}_{\lambda}:\lambda\ge\hat{\lambda}\right\} ,\|\cdot\|_{n}\right)\le A\delta^{-\alpha}
\]

\end_inset


\end_layout

\begin_layout Standard

\series bold
Proof:
\end_layout

\begin_layout Standard
Let 
\begin_inset Formula $I(\hat{g}_{\hat{\lambda}})=R$
\end_inset

.
 We'll suppose 
\begin_inset Formula $R\ge1$
\end_inset

.
 Otherwise, we'll be done.
\end_layout

\begin_layout Standard
By the assumptions, 
\begin_inset Formula $R$
\end_inset

 is bounded since 
\begin_inset Formula 
\[
I^{v}(\hat{g}_{\hat{\lambda}})\le M\|\hat{g}_{\hat{\lambda}}\|_{n}^{2}+M_{0}\le M\left(\|\hat{g}_{\hat{\lambda}}-\hat{g}_{\tilde{\lambda}}\|_{n}+\|\hat{g}_{\tilde{\lambda}}\|_{n}\right)^{2}+M_{0}
\]

\end_inset


\end_layout

\begin_layout Standard
Note that 
\begin_inset Formula $\left\{ \hat{g}_{\lambda}:\lambda\ge\hat{\lambda}\right\} \subseteq\left\{ g:I(g)\le R\right\} =\left\{ Rg:I(g)\le1\right\} $
\end_inset

.
 Note that if 
\begin_inset Formula $h$
\end_inset

 is the closest function in the 
\begin_inset Formula $\delta$
\end_inset

-cover for 
\begin_inset Formula $\left\{ g:I(g)\le1\right\} $
\end_inset

, then
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\|g-h\|_{n}\le\delta\implies\|Rg-\delta\lfloor\frac{R}{\delta}\rfloor h\|_{n}\le\|Rg-Rh\|_{n}+\delta\|h\|_{n}\le\delta(1+R+K)
\]

\end_inset


\end_layout

\begin_layout Standard
Then for some constant 
\begin_inset Formula $\tilde{A}$
\end_inset

 dependent on 
\begin_inset Formula $A$
\end_inset

, 
\begin_inset Formula $R$
\end_inset

, 
\begin_inset Formula $K$
\end_inset

,
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
H\left(\delta,\left\{ \hat{g}_{\lambda}:\lambda\ge\hat{\lambda}\right\} ,\|\cdot\|_{n}\right)\le H\left(\delta,\left\{ Rg:I(g)\le1\right\} ,\|\cdot\|_{n}\right)\le\tilde{A}\delta^{-\alpha}
\]

\end_inset


\end_layout

\begin_layout Subsubsection*
Lemma 4
\end_layout

\begin_layout Standard
Let 
\begin_inset Formula $\hat{\lambda}$
\end_inset

 be the CV-fitted lambda and 
\begin_inset Formula $\tilde{\lambda}$
\end_inset

 be the oracle lambda given in Vandegeer.
 Suppose 
\begin_inset Formula $\hat{\lambda}<\tilde{\lambda}$
\end_inset

.
\end_layout

\begin_layout Standard
Suppose
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
H\left(\delta,\left\{ \hat{g}_{\lambda}:\lambda\in[\hat{\lambda},\tilde{\lambda}]\right\} ,\|\cdot\|_{n}\right)\le A\delta^{-\alpha}
\]

\end_inset


\end_layout

\begin_layout Standard
Suppose
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\left\Vert g^{*}-\hat{g}_{\tilde{\lambda}}\right\Vert _{V}=O_{p}(1)\left\Vert g^{*}-\hat{g}_{\tilde{\lambda}}\right\Vert _{T}
\]

\end_inset


\end_layout

\begin_layout Standard
Then
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\left\Vert \hat{g}_{\tilde{\lambda}}-\hat{g}_{\hat{\lambda}}\right\Vert _{V}=O_{p}(n^{-1/(2+\alpha)})
\]

\end_inset


\end_layout

\begin_layout Standard

\series bold
Proof:
\end_layout

\begin_layout Standard
The basic inequality gives us
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\left\Vert \hat{g}_{\tilde{\lambda}}-\hat{g}_{\hat{\lambda}}\right\Vert _{V}^{2}\le2\left|\left(\epsilon,\hat{g}_{\tilde{\lambda}}-\hat{g}_{\hat{\lambda}}\right)_{V}\right|+2\left|\left(g^{*}-\hat{g}_{\tilde{\lambda}},\hat{g}_{\tilde{\lambda}}-\hat{g}_{\hat{\lambda}}\right)_{V}\right|
\]

\end_inset


\end_layout

\begin_layout Standard

\series bold
Case a:
\series default
 If 
\begin_inset Formula $\left|\left(\epsilon,\hat{g}_{\tilde{\lambda}}-\hat{g}_{\hat{\lambda}}\right)_{T}\right|$
\end_inset

 is the bigger term, then
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\left\Vert \hat{g}_{\tilde{\lambda}}-\hat{g}_{\hat{\lambda}}\right\Vert _{V}^{2}\le O_{P}(1)\left|\left(\epsilon,\hat{g}_{\tilde{\lambda}}-\hat{g}_{\hat{\lambda}}\right)_{V}\right|
\]

\end_inset


\end_layout

\begin_layout Standard
and Thrm 9.1 gives us that 
\begin_inset Formula $\left\Vert \hat{g}_{\tilde{\lambda}}-\hat{g}_{\hat{\lambda}}\right\Vert _{V}=O_{p}(n^{-1/2})$
\end_inset

.
\end_layout

\begin_layout Standard

\series bold
Case b:
\series default
 If 
\begin_inset Formula $\left|\left(g^{*}-\hat{g}_{\tilde{\lambda}},\hat{g}_{\tilde{\lambda}}-\hat{g}_{\hat{\lambda}}\right)_{T}\right|$
\end_inset

 is the bigger term, then by Cauchy Schwarz
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
\left\Vert \hat{g}_{\tilde{\lambda}}-\hat{g}_{\hat{\lambda}}\right\Vert _{V}^{2} & \le & O_{p}(1)\left|\left(g^{*}-\hat{g}_{\tilde{\lambda}},\hat{g}_{\tilde{\lambda}}-\hat{g}_{\hat{\lambda}}\right)_{V}\right|\\
 & \le & O_{p}(1)\left\Vert g^{*}-\hat{g}_{\tilde{\lambda}}\right\Vert _{V}\left\Vert \hat{g}_{\tilde{\lambda}}-\hat{g}_{\hat{\lambda}}\right\Vert _{V}
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
Hence
\begin_inset Formula 
\[
\left\Vert \hat{g}_{\tilde{\lambda}}-\hat{g}_{\hat{\lambda}}\right\Vert _{V}\le O_{p}(n^{-1/(2+\alpha)})
\]

\end_inset


\end_layout

\begin_layout Subsubsection*
Lemma 4 1/4
\end_layout

\begin_layout Standard
Let 
\begin_inset Formula $\hat{\lambda}$
\end_inset

 be the CV-fitted lambda and 
\begin_inset Formula $\tilde{\lambda}$
\end_inset

 be the oracle lambda given in Vandegeer.
 Suppose 
\begin_inset Formula $\hat{\lambda}<\tilde{\lambda}$
\end_inset

.
\end_layout

\begin_layout Standard
Suppose
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
H\left(\delta,\left\{ \frac{g_{1}-g_{2}}{I(g_{1})+I(g_{2})}:g_{1},g_{2}\in\mathcal{G}\right\} ,\|\cdot\|_{n}\right)\le A\delta^{-\alpha}
\]

\end_inset


\end_layout

\begin_layout Standard
Suppose 
\begin_inset Formula 
\[
I^{v}(g)\le M\|g\|_{n}^{2}+M_{0}
\]

\end_inset


\end_layout

\begin_layout Standard
Suppose
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\left\Vert g^{*}-\hat{g}_{\tilde{\lambda}}\right\Vert _{V}=O_{p}(1)\left\Vert g^{*}-\hat{g}_{\tilde{\lambda}}\right\Vert _{T}
\]

\end_inset


\end_layout

\begin_layout Standard
Then
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\left\Vert \hat{g}_{\tilde{\lambda}}-\hat{g}_{\hat{\lambda}}\right\Vert _{V}=O_{p}(n^{-1/(2+\alpha)})
\]

\end_inset


\end_layout

\begin_layout Standard

\series bold
Proof:
\end_layout

\begin_layout Standard
The basic inequality gives us
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\left\Vert \hat{g}_{\tilde{\lambda}}-\hat{g}_{\hat{\lambda}}\right\Vert _{V}^{2}\le2\left|\left(\epsilon,\hat{g}_{\tilde{\lambda}}-\hat{g}_{\hat{\lambda}}\right)_{V}\right|+2\left|\left(g^{*}-\hat{g}_{\tilde{\lambda}},\hat{g}_{\tilde{\lambda}}-\hat{g}_{\hat{\lambda}}\right)_{V}\right|
\]

\end_inset


\end_layout

\begin_layout Standard

\series bold
Case a:
\series default
 If 
\begin_inset Formula $\left|\left(\epsilon,\hat{g}_{\tilde{\lambda}}-\hat{g}_{\hat{\lambda}}\right)_{T}\right|$
\end_inset

 is the bigger term, then
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\left\Vert \hat{g}_{\tilde{\lambda}}-\hat{g}_{\hat{\lambda}}\right\Vert _{V}^{2}\le O_{P}(1)\left|\left(\epsilon,\hat{g}_{\tilde{\lambda}}-\hat{g}_{\hat{\lambda}}\right)_{V}\right|
\]

\end_inset


\end_layout

\begin_layout Standard
By Vandegeer (10.6),
\begin_inset Formula 
\begin{eqnarray*}
\left\Vert \hat{g}_{\tilde{\lambda}}-\hat{g}_{\hat{\lambda}}\right\Vert _{V}^{2} & \le & O_{P}(n^{-1/2})\|\hat{g}_{\tilde{\lambda}}-\hat{g}_{\hat{\lambda}}\|^{1-\alpha/2}\left(I(\hat{g}_{\tilde{\lambda}})+I(\hat{g}_{\hat{\lambda}})\right)^{\alpha/2}
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
If 
\begin_inset Formula $I(\hat{g}_{\tilde{\lambda}})>I(\hat{g}_{\hat{\lambda}})$
\end_inset

, then
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
\left\Vert \hat{g}_{\tilde{\lambda}}-\hat{g}_{\hat{\lambda}}\right\Vert _{V} & \le & O_{P}(n^{-1/(2+\alpha)})I(g^{*})^{\alpha/(2+\alpha)}
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
Otherwise
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
\left\Vert \hat{g}_{\tilde{\lambda}}-\hat{g}_{\hat{\lambda}}\right\Vert _{V} & \le & O_{P}(n^{-1/(2+\alpha)})I(\hat{g}_{\hat{\lambda}})^{\alpha/(2+\alpha)}
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
By assumption,
\begin_inset Formula 
\[
\left(\frac{1}{M}I^{v}\left(\hat{g}_{\tilde{\lambda}}-\hat{g}_{\hat{\lambda}}\right)-\frac{M_{0}}{M}\right)^{1/2}\le\left\Vert \hat{g}_{\tilde{\lambda}}-\hat{g}_{\hat{\lambda}}\right\Vert _{V}\le O_{P}(n^{-1/(2+\alpha)})I(\hat{g}_{\hat{\lambda}})^{\alpha/(2+\alpha)}
\]

\end_inset


\end_layout

\begin_layout Standard
Since 
\begin_inset Formula $I$
\end_inset

 is a psuedonorm,
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\left(I(\hat{g}_{\hat{\lambda}})-I(\hat{g}_{\tilde{\lambda}})\right)^{v}\le I^{v}\left(\hat{g}_{\tilde{\lambda}}-\hat{g}_{\hat{\lambda}}\right)\le O_{P}(n^{-2/(2+\alpha)})I(\hat{g}_{\hat{\lambda}})^{2\alpha/(2+\alpha)}+O_{p}(1)
\]

\end_inset


\end_layout

\begin_layout Standard
Then 
\begin_inset Formula 
\[
I(\hat{g}_{\hat{\lambda}})\le\left(O_{P}(n^{-2/(2+\alpha)})I(\hat{g}_{\hat{\lambda}})^{2\alpha/(2+\alpha)}+O_{p}(1)\right)^{1/v}+O_{p}(1)I(\hat{g}^{*})
\]

\end_inset


\end_layout

\begin_layout Standard
If 
\begin_inset Formula $O_{p}(1)I(\hat{g}^{*})$
\end_inset

 was the largest term, we're done.
\end_layout

\begin_layout Standard
If 
\begin_inset Formula $O_{p}(1)$
\end_inset

 is the larger term, we're done.
\end_layout

\begin_layout Standard
If 
\begin_inset Formula $O_{P}(n^{-2/(2+\alpha)})I(\hat{g}_{\hat{\lambda}})^{2\alpha/(2+\alpha)}$
\end_inset

 is the larger term, then
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
I(\hat{g}_{\hat{\lambda}})\le O_{P}(n^{-2/(2v+\alpha v-2\alpha)})
\]

\end_inset


\end_layout

\begin_layout Standard
Hence 
\begin_inset Formula $I(\hat{g}_{\hat{\lambda}})$
\end_inset

 is bounded and does not grow with 
\begin_inset Formula $n$
\end_inset

.
\end_layout

\begin_layout Standard
Since we have 
\begin_inset Formula 
\[
H\left(\delta,\left\{ g:I(g)\le I(\hat{g}_{\hat{\lambda}})\right\} ,\|\cdot\|_{n}\right)\le\tilde{A}\delta^{-\alpha}
\]

\end_inset


\end_layout

\begin_layout Standard
By Thrm 9.1, then 
\begin_inset Formula $\left\Vert \hat{g}_{\tilde{\lambda}}-\hat{g}_{\hat{\lambda}}\right\Vert _{V}=O_{p}(n^{-1/(2+\alpha)})$
\end_inset

.
\end_layout

\begin_layout Standard

\series bold
Case b:
\series default
 If 
\begin_inset Formula $\left|\left(g^{*}-\hat{g}_{\tilde{\lambda}},\hat{g}_{\tilde{\lambda}}-\hat{g}_{\hat{\lambda}}\right)_{T}\right|$
\end_inset

 is the bigger term, then by Cauchy Schwarz
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
\left\Vert \hat{g}_{\tilde{\lambda}}-\hat{g}_{\hat{\lambda}}\right\Vert _{V}^{2} & \le & O_{p}(1)\left|\left(g^{*}-\hat{g}_{\tilde{\lambda}},\hat{g}_{\tilde{\lambda}}-\hat{g}_{\hat{\lambda}}\right)_{V}\right|\\
 & \le & O_{p}(1)\left\Vert g^{*}-\hat{g}_{\tilde{\lambda}}\right\Vert _{V}\left\Vert \hat{g}_{\tilde{\lambda}}-\hat{g}_{\hat{\lambda}}\right\Vert _{V}
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
Hence
\begin_inset Formula 
\[
\left\Vert \hat{g}_{\tilde{\lambda}}-\hat{g}_{\hat{\lambda}}\right\Vert _{V}\le O_{p}(1)\left\Vert g^{*}-\hat{g}_{\tilde{\lambda}}\right\Vert _{T}\le O_{P}(n^{-1/(2+\alpha)})
\]

\end_inset


\end_layout

\begin_layout Subsubsection*

\series bold
Note:
\end_layout

\begin_layout Standard
I think from Lemma 4 1/4, we have 
\begin_inset Formula 
\[
(\epsilon,\hat{g}_{\hat{\lambda}}-\hat{g}_{\tilde{\lambda}})_{V}\le\|\epsilon_{V}\|\|\hat{g}_{\hat{\lambda}}-\hat{g}_{\tilde{\lambda}}\|\le O_{P}(n^{-1/2-1/(2+\alpha)})
\]

\end_inset


\end_layout

\begin_layout Standard
so we win.
 Fast rates galore.
\end_layout

\begin_layout Standard
\begin_inset Newpage pagebreak
\end_inset


\end_layout

\begin_layout Section*
OLD
\end_layout

\begin_layout Subsubsection*
Lemma 3:
\end_layout

\begin_layout Standard
Suppose the function class 
\begin_inset Formula $\mathcal{F}$
\end_inset

 is bounded s.t.
 
\begin_inset Formula $\sup_{f\in\mathcal{F}}\|f\|_{n}\le R<\infty$
\end_inset

.
 Let 
\begin_inset Formula 
\[
\tilde{\mathcal{F}}=\left\{ \gamma f\mbox{ }:\mbox{ }f\in\mathcal{F},\gamma\in(0,1]\right\} 
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
H\left(\delta(1+R+\delta),\tilde{\mathcal{F}},\|\cdot\|_{n}\right)\le\log(1+\lfloor\frac{1}{\delta}\rfloor)+H\left(\delta,\mathcal{F},\|\cdot\|_{n}\right)
\]

\end_inset


\end_layout

\begin_layout Paragraph*
Proof:
\end_layout

\begin_layout Standard
Let 
\begin_inset Formula $\{h_{i}\}_{i=1}^{N}$
\end_inset

 be the 
\begin_inset Formula $\delta$
\end_inset

-cover for 
\begin_inset Formula $\mathcal{F}$
\end_inset

.
 Consider any 
\begin_inset Formula $f\in\mathcal{F}$
\end_inset

 and let 
\begin_inset Formula $h_{(f)}$
\end_inset

 be the closest function in 
\begin_inset Formula $\delta$
\end_inset

-cover for 
\begin_inset Formula $\mathcal{F}$
\end_inset

.
 Choose 
\begin_inset Formula $j\in\mathbb{Z}^{+}$
\end_inset

 such that 
\begin_inset Formula $|\gamma-\delta j|<\delta$
\end_inset

.
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
\|\gamma f-\delta jh_{(f)}\|_{n} & \le & \|\gamma f-\gamma h_{(f)}\|_{n}+\|\gamma h_{(f)}-\delta jh_{(f)}\|_{n}\\
 & \le & \gamma\|f-h_{(f)}\|_{n}+|\gamma-\delta j|\|h_{(f)}\|_{n}\\
 & \le & \gamma\delta+\delta\left(\|f-h_{(f)}\|_{n}+\|f\|_{n}\right)\\
 & \le & \gamma\delta+\delta\left(\delta+R\right)\\
 & \le & \delta\left(1+R+\delta\right)
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
Hence we have found that the following 
\begin_inset Formula $N(1+\lfloor\frac{1}{\delta}\rfloor)$
\end_inset

 functions form a 
\begin_inset Formula $\delta(1+R+\delta)$
\end_inset

-cover for 
\begin_inset Formula $\tilde{\mathcal{F}}$
\end_inset

:
\begin_inset Formula 
\[
\{h_{i}\}_{i=1}^{N}\cup\left\{ j\delta h_{i}\mbox{ }:\mbox{ }j\in1:\lfloor\frac{1}{\delta}\rfloor,i\in1:N\right\} 
\]

\end_inset


\end_layout

\begin_layout Subsubsection*
Lemma 4:
\end_layout

\begin_layout Standard
Define function classes 
\begin_inset Formula $\{\mathcal{F}_{j}\}_{j=1}^{J}$
\end_inset

 and 
\begin_inset Formula 
\[
\tilde{\mathcal{F}}=\left\{ \sum_{j=1}^{J}f_{j}\mbox{ }:\mbox{ }f_{j}\in\mathcal{F}_{j}\right\} 
\]

\end_inset


\end_layout

\begin_layout Standard
Then
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
H\left(J\delta,\tilde{\mathcal{F}},\|\cdot\|_{n}\right)\le\sum_{j=1}^{J}H\left(\delta,\mathcal{F}_{j},\|\cdot\|_{n}\right)
\]

\end_inset


\end_layout

\begin_layout Paragraph*
Proof:
\end_layout

\begin_layout Standard
For every 
\begin_inset Formula $j=1:J$
\end_inset

, consider any 
\begin_inset Formula $f_{j}\in\mathcal{F}_{j}$
\end_inset

 and let 
\begin_inset Formula $h_{(j)}$
\end_inset

 be the closest function in the 
\begin_inset Formula $\delta$
\end_inset

-cover for 
\begin_inset Formula $\mathcal{F}_{j}$
\end_inset

.
\begin_inset Formula 
\begin{eqnarray*}
\|\sum_{j=1}^{J}f_{j}-\sum_{j=1}^{J}h_{(j)}\| & \le & \sum_{j=1}^{J}\|f_{j}-h_{(j)}\|\le J\delta
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
Hence 
\begin_inset Formula $\exp\left(\sum_{j=1}^{J}H\left(\delta,\mathcal{F}_{j},\|\cdot\|_{n}\right)\right)$
\end_inset

 functions form a 
\begin_inset Formula $J\delta$
\end_inset

-cover for 
\begin_inset Formula $\tilde{\mathcal{F}}$
\end_inset

.
\end_layout

\begin_layout Subsubsection*
Lemma 5:
\end_layout

\begin_layout Standard
Suppose for all 
\begin_inset Formula $j=1,...,J$
\end_inset

, there is some 
\begin_inset Formula $\alpha_{j}>0$
\end_inset

 and 
\begin_inset Formula $A_{j}>0$
\end_inset

 s.t.
 the following entropy bound holds for all 
\begin_inset Formula $\delta>0$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
H\left(\delta,\left\{ \frac{g_{j}-g_{j}^{*}}{I(g_{j})+I(g_{j}^{*})}:g_{j}\in\mathcal{G}_{j},I(g_{j})+I(g_{j}^{*})>0\right\} ,\|\cdot\|_{T}\right)\le A\delta^{-\alpha_{j}}
\]

\end_inset


\end_layout

\begin_layout Standard
Then for sufficiently small
\begin_inset Formula $\delta>0$
\end_inset

, we have
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
H\left(\delta,\left\{ \frac{\sum_{j=1}^{J}g_{j}-g_{j}^{*}}{\sup_{j\in1:J}\left(I(g_{j})+I(g_{j}^{*})\right)}:g_{j}\in\mathcal{G}_{j},I(g_{j})+I(g_{j}^{*})>0\right\} ,\|\cdot\|_{T}\right)\le2JA\left(\frac{\delta}{2J\left(1+R\right)}\right)^{-\alpha_{max}}
\]

\end_inset


\end_layout

\begin_layout Standard
where 
\begin_inset Formula $\alpha_{max}=\max_{j\in1:J}\alpha_{j}$
\end_inset

.
\end_layout

\begin_layout Paragraph*
Proof:
\end_layout

\begin_layout Standard
By Lemma 3,
\begin_inset Formula 
\[
H\left(\delta(1+R+\delta),\left\{ \gamma\frac{g_{j}-g_{j}^{*}}{I(g_{j})+I(g_{j}^{*})}\mbox{ }:\mbox{ }g_{j}\in\mathcal{G}_{j},I(g_{j})+I(g_{j}^{*})>0,\gamma\in(0,1]\right\} ,\|\cdot\|_{T}\right)\le\log(1+\lfloor\frac{1}{\delta}\rfloor)+A\delta^{-\alpha_{j}}
\]

\end_inset


\end_layout

\begin_layout Standard
Note that 
\begin_inset Formula 
\[
\frac{\sum_{j=1}^{J}g_{j}-g_{j}^{*}}{\sup_{j\in1:J}\left(I(g_{j})+I(g_{j}^{*})\right)}=\sum_{j=1}^{J}\left(\frac{I(g_{j})+I(g_{j}^{*})}{\sup_{\ell\in1:J}I(g_{\ell})+I(g_{\ell}^{*})}\right)\frac{g_{j}-g_{j}^{*}}{I(g_{j})+I(g_{j}^{*})}
\]

\end_inset


\end_layout

\begin_layout Standard
By Lemma 4,
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
H\left(J\delta(1+R+\delta),\left\{ \frac{\sum_{j=1}^{J}g_{j}-g_{j}^{*}}{\sup_{j\in1:J}\left(I(g_{j})+I(g_{j}^{*})\right)}\mbox{ }:\mbox{}g_{j}\in\mathcal{G}_{j},I(g_{j})+I(g_{j}^{*})>0\right\} ,\|\cdot\|_{T}\right)\le J\log(1+\lfloor\frac{1}{\delta}\rfloor)+JA\delta^{-\alpha_{j}}
\]

\end_inset


\end_layout

\begin_layout Standard
Hence for sufficiently small 
\begin_inset Formula $\delta$
\end_inset

, 
\begin_inset Formula 
\[
H\left(J\delta(1+R+\delta),\left\{ \frac{\sum_{j=1}^{J}g_{j}-g_{j}^{*}}{\sup_{j\in1:J}\left(I(g_{j})+I(g_{j}^{*})\right)}\mbox{ }:\mbox{}g_{j}\in\mathcal{G}_{j},I(g_{j})+I(g_{j}^{*})>0\right\} ,\|\cdot\|_{T}\right)\le2JA\delta^{-\alpha_{max}}
\]

\end_inset


\end_layout

\begin_layout Standard
Rearranging, we get 
\begin_inset Formula 
\begin{eqnarray*}
H\left(\delta,\left\{ \frac{\sum_{j=1}^{J}g_{j}-g_{j}^{*}}{\sup_{j\in1:J}\left(I(g_{j})+I(g_{j}^{*})\right)}:g_{j}\in\mathcal{G}_{j},I(g_{j})+I(g_{j}^{*})>0\right\} ,\|\cdot\|_{T}\right) & \le & 2AJ\left(\sqrt{\left(\frac{1+R}{2}\right)^{2}+\frac{\delta}{J}}-\frac{1+R}{2}\right)^{-\alpha_{max}}\\
 & \le & 2AJ\left(\frac{\delta}{2J\left(1+R\right)}\right)^{-\alpha_{max}}
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
(Used the fact that for 
\begin_inset Formula $b>0$
\end_inset

 small enough, 
\begin_inset Formula $\sqrt{a^{2}+b}-a\ge\sqrt{(a+\frac{b}{4a})^{2}}-a=\frac{b}{4a}$
\end_inset

)
\end_layout

\begin_layout Subsubsection*
Lemma 5b:
\end_layout

\begin_layout Standard
Suppose for all 
\begin_inset Formula $j=1,...,J$
\end_inset

, there is some 
\begin_inset Formula $\alpha_{j}>0$
\end_inset

 and 
\begin_inset Formula $A_{j}>0$
\end_inset

 s.t.
 the following entropy bound holds for all 
\begin_inset Formula $\delta>0$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
H\left(\delta,\left\{ \frac{g_{j}-g_{j}^{*}}{I(g_{j})+I(g_{j}^{*})}:g_{j}\in\mathcal{G}_{j},I(g_{j})+I(g_{j}^{*})>0\right\} ,\|\cdot\|\right)\le A\delta^{-\alpha_{j}}
\]

\end_inset


\end_layout

\begin_layout Standard
Then for sufficiently small
\begin_inset Formula $\delta>0$
\end_inset

, we have
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
H\left(\delta,\left\{ \frac{\sum_{j=1}^{J}g_{j}-g_{j}^{*}}{\sum_{j=1}^{J}I(g_{j})+I(g_{j}^{*})}:g_{j}\in\mathcal{G}_{j},I(g_{j})+I(g_{j}^{*})>0\right\} ,\|\cdot\|\right)\le2JA\left(STUFF\right)^{-\alpha_{max}}
\]

\end_inset


\end_layout

\begin_layout Standard
where 
\begin_inset Formula $\alpha_{max}=\max_{j\in1:J}\alpha_{j}$
\end_inset

.
\end_layout

\begin_layout Paragraph*
Proof:
\end_layout

\begin_layout Standard
Note that
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\sum_{j=1}^{J}g_{j}-g_{j}^{*}}{\sum_{j=1}^{J}I(g_{j})+I(g_{j}^{*})}=\sum_{j=1}^{J}\left(\frac{I(g_{j})+I(g_{j}^{*})}{\sum_{j=1}^{J}I(g_{\ell})+I(g_{\ell}^{*})}\right)\frac{g_{j}-g_{j}^{*}}{I(g_{j})+I(g_{j}^{*})}
\]

\end_inset


\end_layout

\begin_layout Standard
So we can express
\begin_inset Formula 
\[
\left\{ \frac{\sum_{j=1}^{J}g_{j}-g_{j}^{*}}{\sum_{j=1}^{J}I(g_{j})+I(g_{j}^{*})}:g_{j}\in\mathcal{G}_{j},I(g_{j})+I(g_{j}^{*})>0\right\} \subseteq\left\{ \sum_{j=1}^{J}\gamma_{j}\frac{g_{j}-g_{j}^{*}}{I(g_{j})+I(g_{j}^{*})}:g_{j}\in\mathcal{G}_{j},I(g_{j})+I(g_{j}^{*})>0,\sum_{j=1}^{J}\gamma_{j}=1\right\} 
\]

\end_inset


\end_layout

\begin_layout Standard
Let 
\begin_inset Formula $\mathcal{H}_{j}$
\end_inset

 be the set of functions that form a 
\begin_inset Formula $\delta$
\end_inset

-cover for 
\begin_inset Formula $\left\{ \frac{g_{j}-g_{j}^{*}}{I(g_{j})+I(g_{j}^{*})}:g_{j}\in\mathcal{G}_{j},I(g_{j})+I(g_{j}^{*})>0\right\} $
\end_inset

.
 Consider the set of functions 
\begin_inset Formula 
\[
\left\{ \sum_{j=1}^{J}\delta k_{j}h_{j}\mbox{ }:\mbox{ }h_{j}\in\mathcal{H}_{j},1-\frac{1}{\delta}\le\delta\mbox{\sum}_{j=1}^{J}k_{j}\le1,k_{j}\in1:\lfloor\frac{1}{\delta}\rfloor\right\} 
\]

\end_inset


\end_layout

\begin_layout Standard
Let 
\begin_inset Formula $|\delta k_{j}-\gamma_{i}|<\delta/2$
\end_inset

.
 Then 
\begin_inset Formula 
\begin{eqnarray*}
\left\Vert \sum_{j=1}^{J}\gamma_{j}\frac{g_{j}-g_{j}^{*}}{I(g_{j})+I(g_{j}^{*})}-\sum_{j=1}^{J}\delta k_{j}h_{j}\right\Vert  & \le & \sum_{j=1}^{J}\left\Vert \gamma_{j}\frac{g_{j}-g_{j}^{*}}{I(g_{j})+I(g_{j}^{*})}-\delta k_{j}h_{j}\right\Vert \\
 & \le & \sum_{j=1}^{J}\left\Vert \gamma_{j}\frac{g_{j}-g_{j}^{*}}{I(g_{j})+I(g_{j}^{*})}-\gamma_{i}h_{j}\right\Vert +|\delta k_{j}-\gamma_{i}|\|h_{j}\|\\
 & \le & \sum_{j=1}^{J}\left(\gamma_{j}\delta+\frac{\delta}{2}\left(\left\Vert \frac{g_{j}-g_{j}^{*}}{I(g_{j})+I(g_{j}^{*})}-h_{j}\right\Vert +\left\Vert \frac{g_{j}-g_{j}^{*}}{I(g_{j})+I(g_{j}^{*})}\right\Vert \right)\right)\\
 & \le & \delta(1+JR+J\delta)
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
Hence these 
\begin_inset Formula $\left(\Pi_{j=1}^{J}N_{j}\right){\lfloor\frac{1}{\delta}\rfloor+J-1 \choose J-1}$
\end_inset

 functions form a 
\begin_inset Formula $\delta(1+JR+J\delta)$
\end_inset

 cover.
 Hence the entropy is 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
H\left(\delta(1+JR+J\delta),\left\{ \frac{\sum_{j=1}^{J}g_{j}-g_{j}^{*}}{\sum_{j=1}^{J}I(g_{j})+I(g_{j}^{*})}\mbox{ }:\mbox{}g_{j}\in\mathcal{G}_{j},I(g_{j})+I(g_{j}^{*})>0\right\} ,\|\cdot\|\right)\le(J-1)\log(1+J+\lfloor\frac{1}{\delta}\rfloor)+A\sum_{j=1}^{J}\delta^{-\alpha_{j}}
\]

\end_inset


\end_layout

\begin_layout Standard
Note: 
\begin_inset Formula 
\[
{\lfloor\frac{1}{\delta}\rfloor+J-1 \choose J-1}\le\left(\lfloor\frac{1}{\delta}\rfloor+J-1\right)^{J-1}
\]

\end_inset


\end_layout

\begin_layout Standard
Hence for sufficiently small 
\begin_inset Formula $\delta$
\end_inset

, 
\begin_inset Formula 
\[
H\left(\delta(1+JR+J\delta),\left\{ \frac{\sum_{j=1}^{J}g_{j}-g_{j}^{*}}{\sum_{j=1}^{J}I(g_{j})+I(g_{j}^{*})}\mbox{ }:\mbox{}g_{j}\in\mathcal{G}_{j},I(g_{j})+I(g_{j}^{*})>0\right\} ,\|\cdot\|\right)\le2JA\delta^{-\alpha_{max}}
\]

\end_inset


\end_layout

\begin_layout Standard
Rearranging, we get 
\begin_inset Formula 
\begin{eqnarray*}
H\left(\delta,\left\{ \frac{\sum_{j=1}^{J}g_{j}-g_{j}^{*}}{\sum_{j=1}^{J}I(g_{j})+I(g_{j}^{*})}:g_{j}\in\mathcal{G}_{j},I(g_{j})+I(g_{j}^{*})>0\right\} ,\|\cdot\|\right) & \le & 2AJ\left(\frac{-JR+1+\sqrt{(JR+1)^{2}+4\delta J}}{2J}\right)^{-\alpha_{max}}\\
 & \le & 2AJ\left(\frac{\sqrt{2}\delta J^{3/2}}{1+JR}\right)^{-\alpha_{max}}
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
(Used the fact that for 
\begin_inset Formula $b>0$
\end_inset

 small enough, 
\begin_inset Formula $\sqrt{a^{2}+b}-a\ge\sqrt{(a+\frac{b}{4a})^{2}}-a=\frac{b}{4a}$
\end_inset

)
\end_layout

\begin_layout Subsubsection*
Lemma 6:
\end_layout

\begin_layout Standard
Suppose 
\begin_inset Formula $\epsilon_{i}$
\end_inset

 are sub-gaussian errors and for the function class 
\begin_inset Formula $\mathcal{F}$
\end_inset

, we have that for some 
\begin_inset Formula $0<\alpha<2$
\end_inset

, 
\begin_inset Formula $A'>0$
\end_inset

, and 
\begin_inset Formula $J>0$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
H\left(\delta,\mathcal{F},\|\cdot\|_{T}\right)\le A'J^{\tau}\delta^{-\alpha}\mbox{ }\forall\delta>0
\]

\end_inset


\end_layout

\begin_layout Standard
Then for 
\begin_inset Formula $T=2C_{1}CA'^{1/2}J^{\tau/2}2^{1-\alpha/2}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
Pr\left(\sup_{f\in\mathcal{F}}\frac{\left|\frac{1}{\sqrt{n}}\sum_{i=1}^{n}\epsilon_{i}f(z_{i})\right|}{\|f\|_{n}^{1-\alpha/2}}\ge T\right)\le c\exp(-T^{2}/c^{2})
\]

\end_inset


\end_layout

\begin_layout Paragraph*
Proof:
\end_layout

\begin_layout Standard
Follow proof for Lemma 8.4 in Vandegeer, but with 
\begin_inset Formula $A=A'J^{-\alpha}$
\end_inset

.
 Note that we then have 
\begin_inset Formula $A_{0}=A'^{1/2}J^{\tau/2}$
\end_inset

.
 We then get 
\begin_inset Formula 
\[
Pr\left(\sup_{f\in\mathcal{F}}\frac{\left|\frac{1}{\sqrt{n}}\sum_{i=1}^{n}\epsilon_{i}f(z_{i})\right|}{\|f\|_{n}^{1-\alpha/2}}\ge2C_{1}CA'^{1/2}J^{\tau/2}2^{1-\alpha/2}\right)\le c\exp(-T^{2}/c^{2})
\]

\end_inset


\end_layout

\begin_layout Standard
Note that we can write via shorthand that
\begin_inset Formula 
\[
\sup_{f\in\mathcal{F}}\frac{\left|\frac{1}{n}\sum_{i=1}^{n}\epsilon_{i}f(z_{i})\right|}{\|f\|_{n}^{1-\alpha/2}}=O_{p}(J^{\tau/2}n^{-1/2})
\]

\end_inset


\end_layout

\begin_layout Subsection*
Result 3: Single 
\begin_inset Formula $\lambda$
\end_inset

, Multiple Penalties, Optimal 
\begin_inset Formula $\tilde{\lambda}_{T}$
\end_inset

 over 
\begin_inset Formula $X_{T}$
\end_inset


\end_layout

\begin_layout Standard
Consider function classes 
\begin_inset Formula $\mathcal{G}_{j}$
\end_inset

 that are cones.
 Also, suppose we have an additive model:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
y=\sum_{j=1}^{J}g_{j}^{*}+\epsilon
\]

\end_inset


\end_layout

\begin_layout Standard
where 
\begin_inset Formula $g_{j}^{*}\in\mathcal{G}_{j}$
\end_inset

.
\end_layout

\begin_layout Standard
We fit the model by least squares with separate penalties for each function
 
\begin_inset Formula $g_{j}$
\end_inset

:
\begin_inset Formula 
\[
\{\hat{g}_{j}\}_{j=1}^{J}=\arg\min_{g_{j}\in\mathcal{G}_{j}}\|y-\sum_{j=1}^{J}g_{j}\|_{T}^{2}+\lambda^{2}\sum_{j=1}^{J}I_{j}^{v_{j}}(g_{j})
\]

\end_inset


\end_layout

\begin_layout Standard
Suppose 
\begin_inset Formula $v_{j}\ge1$
\end_inset

 for all 
\begin_inset Formula $j$
\end_inset

.
 (This requirement on 
\begin_inset Formula $v_{j}$
\end_inset

 stricter than Vandegeer Thrm 10.2)
\end_layout

\begin_layout Standard
Suppose for all 
\begin_inset Formula $j$
\end_inset

, there is some 
\begin_inset Formula $0<\alpha<2$
\end_inset

 s.t.
 for all 
\begin_inset Formula $\delta>0$
\end_inset

, 
\begin_inset Formula 
\[
H\left(\delta,\{g_{j}\in\mathcal{G}_{j}:I(g_{j})\le1\},\|\cdot\|_{T}\right)\le A\delta^{-\alpha}
\]

\end_inset


\end_layout

\begin_layout Standard
and that for all 
\begin_inset Formula $j$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\sup_{g_{j}\in\mathcal{G}_{j}}\frac{\|g_{j}\|_{T}}{I(g_{j})}\le R<\infty
\]

\end_inset


\end_layout

\begin_layout Standard
If we choose 
\begin_inset Formula $\lambda$
\end_inset

 s.t.
 
\begin_inset Formula 
\[
\tilde{\lambda}_{T}^{-1}=O_{p}\left(n^{1/(2+\alpha)}\right)\left(J+\sum_{j=1}^{J}I_{j}^{v_{j}}(g_{j}^{*})\right)^{(2-\alpha)/2(2+\alpha)}
\]

\end_inset


\end_layout

\begin_layout Standard
then
\begin_inset Formula 
\[
\|\sum_{j=1}^{J}g_{j}-g_{j}^{*}\|_{T}=O_{p}\left(\tilde{\lambda}_{T}\right)J\left(\sum_{j=1}^{J}I_{j}^{v_{j}}(g_{j}^{*})\right)^{1/2}
\]

\end_inset


\end_layout

\begin_layout Standard
and 
\begin_inset Formula 
\[
\sum_{j=1}^{J}I_{j}(\hat{g}_{j})\le O_{p}(J)\left(J+\sum_{j=1}^{J}I_{j}^{v_{j}}(g_{j}^{*})\right)
\]

\end_inset


\end_layout

\begin_layout Subsubsection*
Proof:
\end_layout

\begin_layout Standard
The basic inequality gives us:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\|\sum_{j=1}^{J}\hat{g}_{j}-g_{j}^{*}\|_{T}^{2}+\lambda^{2}\sum_{j=1}^{J}I_{j}^{v_{j}}(\hat{g}_{j})\le2\left|\left(\epsilon_{T},\sum_{j=1}^{J}\hat{g}_{j}-g_{j}^{*}\right)\right|+\lambda^{2}\sum_{j=1}^{J}I_{j}^{v_{j}}\left(g_{j}^{*}\right)
\]

\end_inset


\end_layout

\begin_layout Standard

\series bold
Case 1: 
\begin_inset Formula $\left|\left(\epsilon_{T},\sum_{j=1}^{J}\hat{g}_{j}-g_{j}^{*}\right)\right|\le\lambda^{2}\sum_{j=1}^{J}I_{j}^{v_{j}}\left(g_{j}^{*}\right)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\|\sum_{j=1}^{J}\hat{g}_{j}-g_{j}^{*}\|_{T}\le O_{p}(\lambda)\left(\sum_{j=1}^{J}I_{j}^{v_{j}}\left(g_{j}^{*}\right)\right)^{1/2}
\]

\end_inset


\end_layout

\begin_layout Standard

\series bold
Case 2: 
\begin_inset Formula $\left|\left(\epsilon_{T},\sum_{j=1}^{J}\hat{g}_{j}-g_{j}^{*}\right)\right|\ge\lambda^{2}\sum_{j=1}^{J}I_{j}^{v_{j}}\left(g_{j}^{*}\right)$
\end_inset


\end_layout

\begin_layout Standard
By Lemma 3,
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
H\left(\delta,\left\{ \frac{\sum_{j=1}^{J}g_{j}-g_{j}^{*}}{\sum_{j=1}^{J}I(g_{j})+I(g_{j}^{*})}:g_{j}\in\mathcal{G}_{j},I(g_{j})+I(g_{j}^{*})>0\right\} ,\|\cdot\|_{T}\right)\le\tilde{A}J^{1-\alpha}\delta^{-\alpha}
\]

\end_inset


\end_layout

\begin_layout Standard
Hence by (10.6) in Vandegeer,
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\sup_{g_{j}\in\mathcal{G}_{j}}\frac{\left|\left(\epsilon_{T},\sum_{j=1}^{J}g_{j}-g_{j}^{*}\right)\right|}{\|\sum_{j=1}^{J}g_{j}-g_{j}^{*}\|^{1-\alpha/2}\left(\sum_{j=1}^{J}I(g_{j})+I(g_{j}^{*})\right)^{\alpha/2}}=O_{p}\left(n^{-1/2}\right)J^{1-\alpha}
\]

\end_inset


\end_layout

\begin_layout Standard
and the basic inequality becomes 
\begin_inset Formula 
\[
\|\sum_{j=1}^{J}\hat{g}_{j}-g_{j}^{*}\|_{T}^{2}+\lambda^{2}\sum_{j=1}^{J}I_{j}^{v_{j}}(\hat{g}_{j})\le O_{p}\left(n^{-1/2}\right)J^{1-\alpha}\|\sum_{j=1}^{J}\hat{g}_{j}-g_{j}^{*}\|_{T}^{1-\alpha/2}\left(\sum_{j=1}^{J}I(\hat{g}_{j})+I(g_{j}^{*})\right)^{\alpha/2}
\]

\end_inset


\end_layout

\begin_layout Standard

\series bold
Case 2a:
\series default
 Suppose 
\begin_inset Formula $\sum_{j=1}^{J}I(\hat{g}_{j})\le\sum I(g_{j}^{*})$
\end_inset

.
 
\end_layout

\begin_layout Standard
Then 
\begin_inset Formula 
\begin{eqnarray*}
\|\sum_{j=1}^{J}\hat{g}_{j}-g_{j}^{*}\|_{T} & \le & O_{p}\left(n^{-1/(2+\alpha)}\right)J^{\frac{2(1-\alpha)}{\alpha+2}}\left(\sum_{j=1}^{J}I(g_{j}^{*})\right)^{\alpha/(2+\alpha)}
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard

\series bold
Case 2b:
\series default
 Suppose 
\begin_inset Formula $\sum_{j=1}^{J}I(\hat{g}_{j})\ge\sum I(g_{j}^{*})$
\end_inset

.
 
\end_layout

\begin_layout Standard
First note that by assuming 
\begin_inset Formula $v_{j}\ge1$
\end_inset

, we must have 
\begin_inset Formula $I_{j}(\hat{g}_{j})\le I_{j}^{v_{j}}(\hat{g}_{j})+1$
\end_inset

.
 So
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
\sum_{j=1}^{J}I_{j}(\hat{g}_{j}) & \le & J+\sum_{j=1}^{J}I_{j}^{v_{j}}(\hat{g}_{j})\\
 & \le & J+O_{p}\left(n^{-1/2}\right)J^{1-\alpha}\lambda^{-2}\|\sum_{j=1}^{J}\hat{g}_{j}-g_{j}^{*}\|_{T}^{1-\alpha/2}\left(\sum_{j=1}^{J}I(\hat{g}_{j})\right)^{\alpha/2}
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard

\series bold
Case 2ba:
\series default
 If the second term on the RHS in the inequality above is bigger, then
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\sum_{j=1}^{J}I_{j}(\hat{g}_{j})\le O_{p}\left(n^{-1/(2-\alpha)}\right)J^{2(1-\alpha)/(2-\alpha)}\lambda^{-4/(2-\alpha)}\|\sum_{j=1}^{J}\hat{g}_{j}-g_{j}^{*}\|_{T}
\]

\end_inset


\end_layout

\begin_layout Standard
which implies
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\|\sum_{j=1}^{J}\hat{g}_{j}-g_{j}^{*}\|_{T}\le O_{p}\left(n^{-1/(2-\alpha)}\right)J^{\alpha(1-\alpha)/(2-\alpha)}\lambda^{-2\alpha/(2-\alpha)}
\]

\end_inset


\end_layout

\begin_layout Standard
and 
\begin_inset Formula 
\[
\sum_{j=1}^{J}I_{j}(\hat{g}_{j})\le J^{(\alpha+2)(1-\alpha)/(2-\alpha)}\left(J+\sum_{j=1}^{J}I_{j}^{v_{j}}(g_{j}^{*})\right)
\]

\end_inset


\end_layout

\begin_layout Standard

\series bold
Case 2bb:
\series default
 If the first term on the RHS in the inequality above is bigger, then
\begin_inset Formula 
\[
\sum_{j=1}^{J}I_{j}(\hat{g}_{j})\le J\implies\|\sum_{j=1}^{J}\hat{g}_{j}-g_{j}^{*}\|_{T}\le O_{p}\left(n^{-1/(2+\alpha)}\right)J^{(2-\alpha)/(2+\alpha)}
\]

\end_inset


\end_layout

\begin_layout Subsection*
Result 5: Multiple 
\begin_inset Formula $\lambda$
\end_inset

, Multiple Penalties, Optimal 
\begin_inset Formula $\lambda$
\end_inset

 on 
\begin_inset Formula $X_{T}$
\end_inset


\end_layout

\begin_layout Standard
Consider an additive model:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
y=\sum_{j=1}^{J}g_{j}^{*}+\epsilon
\]

\end_inset


\end_layout

\begin_layout Standard
We fit the model by least squares with separate penalties and separate 
\begin_inset Formula $\lambda$
\end_inset

 for each function 
\begin_inset Formula $g_{j}$
\end_inset

:
\begin_inset Formula 
\[
\{\hat{g}_{j}\}_{j=1}^{J}=\arg\min_{g_{j}\in\mathcal{G}_{j}}\|y-\sum_{j=1}^{J}g_{j}\|_{T}^{2}+\frac{1}{J}\sum_{j=1}^{J}\lambda_{j}^{2}I_{j}^{v_{j}}(g_{j})
\]

\end_inset


\end_layout

\begin_layout Standard
Suppose 
\begin_inset Formula $v_{j}>\frac{2\alpha_{j}}{2+\alpha_{j}}$
\end_inset

 for all 
\begin_inset Formula $j$
\end_inset

.
\end_layout

\begin_layout Standard
Suppose for all 
\begin_inset Formula $j$
\end_inset

, there is some 
\begin_inset Formula $0<\alpha_{j}<2$
\end_inset

 s.t.
 for all 
\begin_inset Formula $\delta>0$
\end_inset

, 
\begin_inset Formula 
\[
H\left(\delta,\left\{ \frac{g_{j}-g_{j}^{*}}{I(g_{j})+I(g_{j}^{*})}:g_{j}\in\mathcal{G}_{j},I(g_{j})+I(g_{j}^{*})>0\right\} ,\|\cdot\|_{T}\right)\le\frac{A}{J}\delta^{-\alpha_{j}}
\]

\end_inset


\end_layout

\begin_layout Standard
and for all 
\begin_inset Formula $j$
\end_inset

, 
\begin_inset Formula 
\[
\sup_{g_{j}\in\mathcal{G}_{j}}\frac{\|g_{j}-g_{j}^{*}\|_{T}}{I(g_{j})+I(g_{j}^{*})}\le R<\infty
\]

\end_inset


\end_layout

\begin_layout Standard
If we choose 
\begin_inset Formula $\lambda$
\end_inset

 s.t.
 
\begin_inset Formula 
\[
\tilde{\lambda}_{j}^{-1}=???
\]

\end_inset


\end_layout

\begin_layout Standard
then
\begin_inset Formula 
\[
\|\sum_{j=1}^{J}g_{j}-g_{j}^{*}\|_{T}=???
\]

\end_inset


\end_layout

\begin_layout Standard
and 
\begin_inset Formula 
\[
\sum_{j=1}^{J}I^{v_{j}}(\hat{g}_{\lambda,j})=???
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Newpage pagebreak
\end_inset


\end_layout

\begin_layout Subsubsection*
Example 1: Sobelov norm (NOT DONE)
\end_layout

\begin_layout Standard
Consider the functions 
\begin_inset Formula 
\[
\mathcal{G}=\left\{ g:[0,1]\mapsto\mathbb{R}:\int_{0}^{1}g^{(m)}(z)^{2}dz<\infty\right\} 
\]

\end_inset


\end_layout

\begin_layout Standard
Suppose 
\begin_inset Formula $x_{i}$
\end_inset

 are all unique.
 Then the Sobelov norm for the class 
\begin_inset Formula $\{\hat{g}_{\lambda}\in\mathcal{G}:\lambda\in\Lambda\}$
\end_inset

 is bounded above by its 
\begin_inset Formula $L_{2}(P_{n})$
\end_inset

 norm.
 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
I^{2}(\hat{g}_{\lambda})=\int_{0}^{1}\left(\hat{g}_{\lambda}^{(m)}(z)\right)^{2}dz\le2\|\hat{g}_{\lambda}\|_{n}^{2}+4I^{2}(\tilde{g})+4\|y\|_{n}^{2}\mbox{ }\forall\lambda\in\Lambda
\]

\end_inset


\end_layout

\begin_layout Standard
PROBLEM: as defined, it is possible that 
\begin_inset Formula $I^{2}(\tilde{g})$
\end_inset

 grows with 
\begin_inset Formula $n$
\end_inset

, which is not okay!
\end_layout

\begin_layout Standard

\series bold
Proof:
\end_layout

\begin_layout Standard
Let 
\begin_inset Formula $\tilde{g}$
\end_inset

 satisfy 
\begin_inset Formula $\tilde{g}(x_{i})=y_{i}$
\end_inset

 and have the smallest value for 
\begin_inset Formula $\int_{0}^{1}\left(\tilde{g}^{(m)}(z)\right)^{2}dz$
\end_inset

.
 This function 
\begin_inset Formula $\tilde{g}$
\end_inset

 should always exist.
 
\end_layout

\begin_layout Standard

\series bold
Case 1:
\series default
 
\begin_inset Formula $\lambda\le1/2$
\end_inset


\end_layout

\begin_layout Standard
By definition of 
\begin_inset Formula $\hat{g}_{\lambda}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\|y-\hat{g}_{\lambda}\|_{n}^{2}+\lambda^{2}I^{2}(\hat{g}_{\lambda})\le\|y-(\tilde{g}-\lambda\hat{g}_{\lambda})\|_{n}^{2}+\lambda^{2}I^{2}(\tilde{g}-\lambda\hat{g}_{\lambda})
\]

\end_inset


\end_layout

\begin_layout Standard
Note that 
\begin_inset Formula 
\begin{eqnarray*}
I^{2}(\tilde{g}-\lambda\hat{g}_{\lambda}) & = & \int_{0}^{1}\left(\tilde{g}^{(m)}-\lambda\hat{g}_{\lambda}^{(m)}\right)^{2}dz\\
 & = & 2\int_{0}^{1}\max\left(\left|\tilde{g}^{(m)}\right|^{2},\left|\lambda\hat{g}_{\lambda}^{(m)}\right|^{2}\right)dz\\
 & = & 2\left(\int_{0}^{1}\left|\tilde{g}^{(m)}\right|^{2}dz+\int_{0}^{1}\left|\lambda\hat{g}_{\lambda}^{(m)}\right|^{2}dz\right)
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
Hence
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\lambda^{2}I^{2}(\hat{g}_{\lambda})\le\lambda^{2}\|\hat{g}_{\lambda}\|_{n}^{2}+2\lambda^{2}I^{2}(\tilde{g})+2\lambda^{4}I^{2}(\hat{g}_{\lambda})
\]

\end_inset


\end_layout

\begin_layout Standard
The following ineq follows, where the RHS is maximized when 
\begin_inset Formula $\lambda=1/2$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
I^{2}(\hat{g}_{\lambda})\le\frac{\lambda^{2}}{\lambda^{2}-2\lambda^{4}}\left(\|\hat{g}_{\lambda}\|_{n}^{2}+2I^{2}(\tilde{g})\right)\le2\|\hat{g}_{\lambda}\|_{n}^{2}+4I^{2}(\tilde{g})
\]

\end_inset


\end_layout

\begin_layout Standard

\series bold
Case 2:
\series default
 
\begin_inset Formula $\lambda>1/2$
\end_inset


\end_layout

\begin_layout Standard
By definition of 
\begin_inset Formula $\hat{g}_{\lambda}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\|y-\hat{g}_{\lambda}\|_{n}^{2}+\lambda^{2}I^{2}(\hat{g}_{\lambda})\le\|y\|_{n}^{2}
\]

\end_inset


\end_layout

\begin_layout Standard
The RHS is maximized when 
\begin_inset Formula $\lambda=1/2$
\end_inset

, so
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
I^{2}(\hat{g}_{\lambda})\le4\|y\|_{n}^{2}
\]

\end_inset


\end_layout

\begin_layout Standard
Hence we have an upper bound for the Sobelov norm
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
I^{2}(\hat{g}_{\lambda})\le2\|\hat{g}_{\lambda}\|_{n}^{2}+4I^{2}(\tilde{g})+4\|y\|_{n}^{2}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Newpage pagebreak
\end_inset


\end_layout

\begin_layout Subsubsection*
Appendix
\end_layout

\begin_layout Paragraph

\series bold
A cute lemma I found but never used:
\series default
 
\end_layout

\begin_layout Standard
Supposing that 
\begin_inset Formula $I^{v}(\hat{g}_{\lambda})$
\end_inset

 is continuous in 
\begin_inset Formula $\lambda$
\end_inset

, then given training data 
\begin_inset Formula $T$
\end_inset

, 
\begin_inset Formula 
\begin{eqnarray*}
\frac{\partial}{\partial\lambda}L_{T}(\hat{g}_{\lambda},\lambda) & = & 2\lambda I^{v}(\hat{g}_{\lambda})
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
Also, 
\begin_inset Formula $L_{T}$
\end_inset

 is convex in 
\begin_inset Formula $\lambda$
\end_inset

.
\end_layout

\begin_layout Standard

\series bold
Proof:
\end_layout

\begin_layout Standard
By definition, 
\begin_inset Formula 
\[
L_{T}(\hat{g}_{\lambda},\lambda)=\|y-\hat{g}_{\lambda}\|_{T}^{2}+\lambda^{2}I^{v}(\hat{g}_{\lambda})\le\|y-\hat{g}_{\lambda'}\|_{T}^{2}+\lambda^{2}I^{v}(\hat{g}_{\lambda'})=L_{T}(\hat{g}_{\lambda'},\lambda)
\]

\end_inset


\end_layout

\begin_layout Standard
Then we can provide upper and lower bounds for 
\begin_inset Formula $L_{T}(\hat{g}_{\lambda_{2}},\lambda_{2})-L_{T}(\hat{g}_{\lambda_{1}},\lambda_{1})$
\end_inset

: 
\begin_inset Formula 
\begin{eqnarray*}
L_{T}(\hat{g}_{\lambda_{2}},\lambda_{2})-L_{T}(\hat{g}_{\lambda_{1}},\lambda_{1}) & \le & L_{T}(\hat{g}_{\lambda_{1}},\lambda_{2})-L_{T}(\hat{g}_{\lambda_{1}},\lambda_{1})\\
 & = & \|y-\hat{g}_{\lambda_{1}}\|_{T}^{2}+\lambda_{2}^{2}I^{v}(\hat{g}_{\lambda_{1}})-\|y-\hat{g}_{\lambda_{1}}\|_{T}^{2}-\lambda_{1}^{2}I^{v}(\hat{g}_{\lambda_{1}})\\
 & = & (\lambda_{2}^{2}-\lambda_{1}^{2})I^{v}(\hat{g}_{\lambda_{1}})
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
L_{T}(\hat{g}_{\lambda_{2}},\lambda_{2})-L_{T}(\hat{g}_{\lambda_{1}},\lambda_{1}) & \ge & L_{T}(\hat{g}_{\lambda_{2}},\lambda_{2})-L_{T}(\hat{g}_{\lambda_{2}},\lambda_{1})\\
 & = & \|y-\hat{g}_{\lambda_{2}}\|_{T}^{2}+\lambda_{2}^{2}I^{v}(\hat{g}_{\lambda_{2}})-\|y-\hat{g}_{\lambda_{2}}\|_{T}^{2}-\lambda_{1}^{2}I^{v}(\hat{g}_{\lambda_{2}})\\
 & = & (\lambda_{2}^{2}-\lambda_{1}^{2})I^{v}(\hat{g}_{\lambda_{2}})
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
So suppose WLOG 
\begin_inset Formula $\lambda_{2}>\lambda_{1}$
\end_inset

:
\begin_inset Formula 
\[
(\lambda_{2}+\lambda_{1})I^{v}(\hat{g}_{\lambda_{2}})\le\frac{L_{T}(\hat{g}_{\lambda_{2}},\lambda_{2})-L_{T}(\hat{g}_{\lambda_{1}},\lambda_{1})}{\lambda_{2}-\lambda_{1}}\le(\lambda_{2}+\lambda_{1})I^{v}(\hat{g}_{\lambda_{1}})
\]

\end_inset


\end_layout

\begin_layout Standard
So as 
\begin_inset Formula $\lambda_{1}\rightarrow\lambda_{2}=\lambda$
\end_inset

, we have by the sandwich theorem,
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial}{\partial\lambda}L_{T}(\hat{g}_{\lambda},\lambda)=2\lambda I^{v}(\hat{g}_{\lambda})
\]

\end_inset


\end_layout

\begin_layout Standard
Furthermore, given training data 
\begin_inset Formula $T$
\end_inset

 
\begin_inset Formula 
\[
\frac{\partial}{\partial\lambda}L_{T}(\hat{g}_{\lambda},\lambda)=\frac{\partial}{\partial\lambda}\|y-\hat{g}_{\lambda}\|_{T}^{2}+2\lambda I^{v}(\hat{g}_{\lambda})+\lambda^{2}\frac{\partial}{\partial\lambda}I^{v}(\hat{g}_{\lambda})
\]

\end_inset


\end_layout

\begin_layout Standard
then, combining this with the lemma, we have that 
\begin_inset Formula 
\[
\frac{\partial}{\partial\lambda}\|y-\hat{g}_{\lambda}\|_{T}^{2}=-\lambda^{2}\frac{\partial}{\partial\lambda}I^{v}(\hat{g}_{\lambda})
\]

\end_inset


\end_layout

\begin_layout Standard
Finally, to see that 
\begin_inset Formula $L_{T}$
\end_inset

 is convex in 
\begin_inset Formula $\lambda$
\end_inset

, note that 
\begin_inset Formula 
\[
\frac{\partial^{2}}{\partial\lambda^{2}}L_{T}(\hat{g}_{\lambda},\lambda)=2I^{v}(\hat{g}_{\lambda})+2\lambda vI^{v-1}(\hat{g}_{\lambda})\frac{\partial}{\partial\lambda}I(\hat{g}_{\lambda})>0
\]

\end_inset


\end_layout

\begin_layout Standard
since 
\begin_inset Formula $\frac{\partial}{\partial\lambda}I(\hat{g}_{\lambda})>0$
\end_inset

.
\end_layout

\end_body
\end_document
